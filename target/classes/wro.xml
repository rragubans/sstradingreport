<?xml version="1.0" encoding="UTF-8"?>

<groups xmlns="http://www.isdc.ro/wro">
    <!-- OSM / Openlayer Dependencies -->
    <group name="osm">
        <css>/webjars/openlayers/2.13.1/theme/default/style.css</css>
        <js minimize="false">/webjars/openlayers/2.13.1/OpenLayers.js</js>
        <js>/js/OpenStreetMap.js</js>
        <js>/js/OpenLayersDeprecated.js</js>
        <js>/js/OpenLayersConfig.js</js>
    </group>

    <group name="sstradingreport-embedded">
        <group-ref>osm</group-ref>

        <js minimize="false">/webjars/jquery/2.1.1/jquery.min.js</js>
        <js minimize="false">/webjars/angularjs/1.4.5/angular.min.js</js>
        <js>/js/directives.js</js>
    </group>

    <!-- Dependencies for the full site -->
    <group name="sstradingreport">
        <group-ref>osm</group-ref>

        <css minimize="false">/webjars/bootstrap/3.1.1/css/bootstrap.min.css</css>
        <css>/css/stylesheet.css</css>

        <js minimize="false">/webjars/jquery/2.1.1/jquery.min.js</js>
        <js minimize="false">/webjars/bootstrap/3.1.1/js/bootstrap.min.js</js>
        <js minimize="false">/webjars/momentjs/2.10.6/min/moment-with-locales.min.js</js>
        <js minimize="false">
            /webjars/angular-file-upload/1.6.12/angular-file-upload-html5-shim.min.js
        </js>
        <js minimize="false">/webjars/angularjs/1.4.5/angular.min.js</js>
        <js minimize="false">/webjars/angularjs/1.4.5/angular-route.min.js</js>
        <js minimize="false">/webjars/angular-file-upload/1.6.12/angular-file-upload.min.js</js>
        <js minimize="false">/webjars/angular-ui-bootstrap/0.11.2/ui-bootstrap.min.js</js>
        <js minimize="false">/webjars/angular-ui-bootstrap/0.11.2/ui-bootstrap-tpls.min.js</js>
        <js minimize="false">/webjars/highcharts/@highcharts.version@/highcharts.js</js>
        <js minimize="false">/webjars/highcharts/@highcharts.version@/highcharts-more.js</js>
        <js minimize="false">/webjars/sockjs-client/1.0.2/sockjs.min.js</js>
        <js minimize="false">/webjars/stomp-websocket/2.3.3/stomp.min.js</js>

        <js>/js/app.js</js>
        <js>/js/controllers.js</js>
        <js>/js/directives.js</js>
    </group>
</groups>